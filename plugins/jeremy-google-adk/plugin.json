{
  "name": "jeremy-google-adk",
  "version": "1.0.0",
  "description": "Google Agent Development Kit (ADK) SDK starter kit for building Claude-powered AI agents with React patterns, multi-agent orchestration, and tool integration",
  "author": {
    "name": "Jeremy Longshore",
    "email": "jeremy@claudecodeplugins.io",
    "url": "https://github.com/jeremylongshore"
  },
  "repository": "https://github.com/jeremylongshore/jeremy-google-adk",
  "license": "MIT",
  "keywords": [
    "google",
    "adk",
    "agent-development-kit",
    "sdk",
    "ai-agents",
    "claude",
    "react-pattern",
    "multi-agent",
    "orchestration",
    "tools",
    "framework",
    "scaffolding",
    "starter-kit"
  ],
  "skills": [
    "skills/adk-agent-builder"
  ],
  "commands": [
    "slash-commands/create-adk-agent.md",
    "slash-commands/scaffold-multi-agent.md",
    "slash-commands/generate-tools.md"
  ],
  "requirements": [
    "Claude Code 1.0+",
    "Python 3.11+",
    "Google Cloud SDK",
    "anthropic>=0.18.0"
  ],
  "mcp_servers": [{
    "name": "google-adk",
    "command": "python",
    "args": ["-m", "google_adk.mcp_server"],
    "env": {
      "CLAUDE_API_KEY": "${CLAUDE_API_KEY}",
      "GOOGLE_PROJECT_ID": "${GOOGLE_PROJECT_ID}"
    }
  }]
}