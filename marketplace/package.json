{
  "name": "marketplace",
  "type": "module",
  "version": "3.0.0",
  "scripts": {
    "dev": "astro dev",
    "build": "npm run skills:generate && npm run search:generate && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "skills:generate": "node scripts/discover-skills.mjs",
    "search:generate": "node scripts/generate-unified-search.mjs",
    "validate:routes": "node scripts/validate-routes.mjs",
    "validate:playbooks": "node scripts/validate-playbook-routes.mjs",
    "validate:internal-links": "node scripts/validate-internal-links.mjs",
    "validate": "npm run validate:playbooks && npm run validate:routes",
    "validate:dist": "npm run validate:playbooks -- --dist && npm run validate:internal-links"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.16.6",
    "fuse.js": "^7.1.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0"
  }
}
