    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent" />
    <meta name="apple-mobile-web-app-title" content="Claude Code Plugins Sponsor" />

    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />

